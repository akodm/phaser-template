{
  "name": "phaser-template",
  "description": "A Phaser 3 TypeScript template using Vite.",
  "version": "0.0.1",
  "engines": {
    "node": ">=22.11.0",
    "yarn": ">=1.22.11"
  },
  "scripts": {
    "dev": "vite --config vite/config.dev.mjs",
    "build": "yarn lint && vite build --config vite/config.prod.mjs",
    "lint": "eslint . --ext .ts,.js --cache --fix"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "phaser": "^3.90.0",
    "terser": "^5.39.0"
  }
}
